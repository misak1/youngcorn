    "express": "^4.13.0",
    "glob": "^5.0.10",
    "iterate79": "0.0.1",
    "marked": "^0.3.3",
    "mkdirp": "0.*",
    "node-php-bin": "^0.1.6",
    "phpjs": "^1.3.2",
    "px2agent": "^2.0.2",
    "px2dt-localdata-access": "git://github.com/tomk79/node-px2dt-localdata-access.git",
    "rmdir": "^1.1.0",
    "socket.io": "^1.3.5",
    "twig": "^0.8.2"
  },
  "devDependencies": {
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-browserify": "^0.5.1",
